<div class="parallax-container">
  <div class="parallax"><img src="<%= attributes.image %>"></div>
</div>

<div class="section container">
  <div class="row">
    <div class="col s12 m4 l4">
      <h6><i class="mdi-av-loop small left"></i><%= attributes.status %></h6>
    </div>
    <div class="col s12 m4 l4">
      <h6><i class="mdi-maps-place small left"></i><%= attributes.location %></h6>
    </div>
    <div class="col s12 m4 l4">
      <h6><i class="mdi-content-link small left"></i><a href="<%= attributes.website %>">Website</a></h6>
    </div>
  </div>

  <h4 class="truncate"><%= attributes.title %></h4>
  <p class="flow-text"><%= attributes.description %></p>
  <div class="row">
    <div class="col s12 m6 l6">
      <h6>Primary Department</h6>
      <h5><%= attributes.primary_department %><h5>
    </div>
    <div class="col s12 m6 l6">
      <h6>Project Manager</h6>
      <h5><%= attributes.project_manager %></h5>
    </div>
  </div>
</div>

<div class="pushpin">
  <a href="<%= links.prev %>" class="btn-floating btn-large waves-effect waves-light left indigo darken-4 lighten-1 previous-article"><i class="mdi-navigation-chevron-left"></i></a>
  <a href="<%= links.next %>" class="btn-floating btn-large waves-effect waves-light right indigo darken-4 lighten-1 next-article"><i class="mdi-navigation-chevron-right"></i></a>
</div>