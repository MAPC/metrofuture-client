<div class="parallax-container">
  <div class="parallax"><img src="<%= attributes.image %>"></div>
</div>

<div class="section container">
  <div class="row">
    <div class="col s12 m4 l4">
      <h6><i class="small left <%= this.slugify(attributes.status) %>"></i><%= attributes.status %></h6>
    </div>

    <% if (attributes.website !== null) { %>
      <div class="col s12 m4 l4">
        <h6><i class="mdi-content-link small left"></i><a href="<%= attributes.website %>">Website</a></h6>
      </div>
    <% } %>
  </div>

  <h4><%= attributes.title %></h4>
  <p class="flow-text"><%= attributes.description %></p>
  <div class="row">
    <div class="col s12 m6 l6">
      <h6>Primary Department</h6>
      <h5><%= attributes.primary_department %><h5>
    </div>
    <div class="col s12 m6 l6">
      <h6>Project Manager</h6>
      <h5><%= attributes.project_manager %></h5>
    </div>
  </div>
</div>

<div class="section container">
  <i class="mdi-maps-place small left"></i><h6>Municipalities</h6>
  <div class="">
    <% included.forEach(function(municipality, index) { %>
      <a href="projects?municipalities=<%= municipality.attributes.muni_id %>" class="collection-item">
        <%= municipality.attributes.name %></a><% if (index < included.length - 1) { %>, <% } %>
      

      
    <% }) %>
  </div>
</div>

<div class="section container">
  <i class="mdi-maps-place small left"></i><h6>Subregions</h6>
  <div class="">
      <a href="http://regionalindicators.live.mapc.org/goals/" class="collection-item">
        Boston
      </a>,
            <a href="http://regionalindicators.live.mapc.org/goals/" class="collection-item">
        Boston
      </a>,
            <a href="http://regionalindicators.live.mapc.org/goals/" class="collection-item">
        Boston
      </a>,
            <a href="http://regionalindicators.live.mapc.org/goals/" class="collection-item">
        Boston
      </a>,
            <a href="http://regionalindicators.live.mapc.org/goals/" class="collection-item">
        Boston
      </a>
  </div>
</div>


<div class="section container">
  <h6>MetroFuture Goals</h6>
