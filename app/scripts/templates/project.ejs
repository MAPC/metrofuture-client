<div class="pushpin">
  <% if (links.prev !== null) { %>
    <a href="#<%= this.scrubLink(links.prev, MfiaClient.API) %>" class="pushpin btn-floating btn-large waves-effect waves-light left indigo darken-4 lighten-1 previous-article tooltipped" data-position="right" data-delay="50" data-tooltip="Previous Project"><i class="mdi-navigation-chevron-left"></i></a>
  <% } %>
  <% if (links.next !== null) { %>
    <a href="#<%= this.scrubLink(links.next, MfiaClient.API) %>" class="pushpin btn-floating btn-large waves-effect waves-light right indigo darken-4 lighten-1 next-article tooltipped" data-position="left" data-delay="50" data-tooltip="Next Project"><i class="mdi-navigation-chevron-right"></i></a>
  <% } %>
</div>